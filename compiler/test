Bob [
  knows private Kab
]

Alice [
  knows private Kab
  generates Na
  m1 = ENC(Kab, Na)
  m0 = HASH(Alice, Kab)
]
Alice -> Bob: {m0, m1}

Bob [
  ASSERT(m0, HASH(Alice, Kab))?
  Na = DEC(Kab, m1)
  generates Nb
  m2 = ENC(Kab, Nb)
]
Bob -> Alice: Bob, m2

Alice [
  Nb = DEC(Kab, m2)
]

queries [
  executable?
]