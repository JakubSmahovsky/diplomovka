rule init: [
]-->[
!Alice_init(),
!Bob_init()
]

rule Alice_0: [
!Alice_init()
]--[
Alice_1()
]->[
Alice_1()
]

rule Alice_1: [
Alice_1(),
In($Bob),
In($Alice),
In($X)
]--[
Alice_2($Bob,$Alice,$X)
]->[
Alice_2($Bob,$Alice,$X)
]

rule Bob_0: [
!Bob_init()
]--[
Bob_1($X)
]->[
Out($Bob),
Out($Alice),
Out($X),
Bob_1($X)
]

lemma executable:
exists-trace "
Ex Alice #t1 #t0 Bob X.
Alice_2($Bob,$Alice,$X)@ #t0 &
Bob_1($X)@ #t1
"
